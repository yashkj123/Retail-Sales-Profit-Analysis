SELECT * FROM superstore LIMIT 5;

--to check for the number of rows :
SELECT COUNT(*) AS total_rows FROM superstore;

--to find table sales and profit :
SELECT
    ROUND(SUM(Sales), 2) AS total_sales,
    ROUND(SUM(Profit), 2) AS total_profit
FROM
    superstore;
	
	
--Region wise sales and profit analysis 
SELECT
    Region,
    ROUND(SUM(Sales), 2) AS total_sales,
    ROUND(SUM(Profit), 2) AS total_profit
FROM
    superstore
GROUP BY
    Region
ORDER BY
    total_profit DESC;
	
-- Category + Sub-Category Profit Analysis:	
SELECT
    Category,
    [Sub-Category] AS sub_category,
    ROUND(SUM(Sales), 2) AS total_sales,
    ROUND(SUM(Profit), 2) AS total_profit
FROM
    superstore
GROUP BY
    Category, [Sub-Category]
ORDER BY
    total_profit DESC; --â†’ Highest profit products first
	
-- âœ… Try This Too (Bottom Performers):
-- To find lowest profit products (maybe even negative profit):
SELECT
    Category,
    [Sub-Category] AS sub_category,
    ROUND(SUM(Profit), 2) AS total_profit
FROM
    superstore
GROUP BY
    Category, [Sub-Category]
ORDER BY
    total_profit ASC
LIMIT 5;

-- Customer Segmentation Analysis--&gt;
--âœ… Query: Top 10 Customers by Profit
SELECT
    [Customer Name] AS customer,
    ROUND(SUM(Sales), 2) AS total_sales,
    ROUND(SUM(Profit), 2) AS total_profit
FROM
    superstore
GROUP BY
    [Customer Name]
ORDER BY
    total_profit DESC
LIMIT 10;

--âœ… Bonus Query: Bottom 5 Customers (for comparison)
SELECT
    [Customer Name] AS customer,
    ROUND(SUM(Sales), 2) AS total_sales,
    ROUND(SUM(Profit), 2) AS total_profit
FROM
    superstore
GROUP BY
    [Customer Name]
ORDER BY
    total_profit ASC
LIMIT 5;

--Monthly Sales Trend Analysis
--Date Format Check :Before analyzing time-based data, we need to know the format of the Order Date column.
SELECT [Order Date] FROM superstore LIMIT 5;
--ðŸ”§ SQLite Doesn't Have a DATE Column Type by Default :So we'll need to extract Year and Month manually using string functions.
--âœ… Query: Monthly Sales &amp; Profit Trends
SELECT
    SUBSTR([Order Date], 7, 4) AS year,         -- Characters 7â€“10 = YYYY
    SUBSTR([Order Date], 1, 2) AS month,        -- Characters 1â€“2 = MM
    ROUND(SUM(Sales), 2) AS total_sales,
    ROUND(SUM(Profit), 2) AS total_profit
FROM
    superstore
GROUP BY
    year, month
ORDER BY
    year, month;
	
--State-Level Loss Analysis
SELECT
    State,
    ROUND(SUM(Sales), 2) AS total_sales,
    ROUND(SUM(Profit), 2) AS total_profit
FROM
    superstore
GROUP BY
    State
HAVING total_profit &lt; 0
ORDER BY total_profit ASC;

--âœ… Bonus Query: All States Ordered by Profit
SELECT
    State,
    ROUND(SUM(Sales), 2) AS total_sales,
    ROUND(SUM(Profit), 2) AS total_profit
FROM
    superstore
GROUP BY
    State
ORDER BY
    total_profit ASC;



	






